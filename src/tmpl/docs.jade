extends layout

block content
  .container
    .row-fluid
      .span3
        .well.sidebar-nav
          h2.nav-header Documentation
          ul.nav.nav-list
            each section in sections
              li
                a(href='/' + page + '/' + section.url)= section.name
      .span9.page
        .hero-unit
          h1= title
          p!= content
